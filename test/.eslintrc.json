{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "templateStrings": true,
      "modules": true,
      "destructuring": true,
      "generators": true,
      "experimentalObjectRestSpread": true,
      "impliedStrict": true
    }
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "globals": {
    "__DEV__": false
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "consistent-return": 2,
    "no-whitespace-before-property": 2,
    "no-unneeded-ternary": 2,
    "max-len": [0, 120],
    "max-lines": [0, 500],
    "consistent-this": 2,
    "keyword-spacing": 2,
    "block-spacing": 2,
    "max-depth": 2,
    "brace-style": 2,
    "comma-style": 2,
    "comma-spacing": 2,
    "camelcase": 2,
    "operator-linebreak": 2,
    "no-console": 0,
    "no-var": 2,
    "no-tabs": 2,
    "no-restricted-syntax": ["error", "WithStatement"],
    "no-underscore-dangle": 2,
    "space-before-blocks": 2,
    "spaced-comment": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "no-useless-rename": 2,
    "prefer-const": 2,
    "no-const-assign": 2,
    "no-duplicate-imports": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "generator-star-spacing": 2,
    "prefer-spread": 2,
    "quotes": [0, "single"],
    "prefer-rest-params": 2,
    "rest-spread-spacing": 2,
    "require-yield": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-lone-blocks": 2,
    "space-unary-ops": 2,
    "guard-for-in": 2,
    "no-redeclare": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-return-assign": 2,
    "no-unmodified-loop-condition": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "radix": 2,
    "wrap-iife": 2,
    "arrow-spacing": 2,
    "no-multi-spaces": 2,
    "complexity": 2,
    "yoda": 2,
    "no-unused-vars": 2,
    "no-undefined": 0,
    "no-use-before-define": 2,
    "no-restricted-globals": 2,
    "object-shorthand": [0, "methods"],
    "indent": [0, 2],
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2,
    "react-native/no-inline-styles": 2,
    "react-native/no-color-literals": 0,
    "react/display-name": 0
  }
}
